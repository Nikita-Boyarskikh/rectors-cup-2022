{
  "masterPicture": "./public/logo.png",
  "iconsPath": "/",
  "design": {
    "ios": {
      "pictureAspect": "backgroundAndMargin",
      "backgroundColor": "#FCD800",
      "margin": "21%",
      "assets": {
        "ios6AndPriorIcons": true,
        "ios7AndLaterIcons": true,
        "precomposedIcons": true,
        "declareOnlyDefaultIcon": true
      },
      "appName": "Кубок ректора 2023"
    },
    "desktopBrowser": {
      "design": "raw"
    },
    "windows": {
      "pictureAspect": "noChange",
      "backgroundColor": "#FFC40D",
      "onConflict": "override",
      "assets": {
        "windows80Ie10Tile": true,
        "windows10Ie11EdgeTiles": {
          "small": true,
          "medium": true,
          "big": true,
          "rectangle": true
        }
      },
      "appName": "Кубок ректора 2023"
    },
    "androidChrome": {
      "pictureAspect": "noChange",
      "themeColor": "#FCD800",
      "manifest": {
        "name": "Кубок ректора 2023",
        "display": "standalone",
        "orientation": "notSet",
        "onConflict": "override",
        "declared": true
      },
      "assets": {
        "legacyIcon": false,
        "lowResolutionIcons": true
      }
    },
    "safariPinnedTab": {
      "pictureAspect": "silhouette",
      "themeColor": "#FCD800"
    }
  },
  "settings": {
    "compression": 5,
    "scalingAlgorithm": "Cubic",
    "errorOnImageTooSmall": false,
    "readmeFile": true,
    "htmlCodeFile": true,
    "usePathAsIs": false
  }
}
